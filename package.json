{
	"name": "lmntal",
	"displayName": "LMNtal",
	"description": "LMNtal language support for Visual Studio Code",
	"icon": "images/icon.svg",
	"version": "0.0.1",
	"publisher": "remin",
	"engines": {
		"vscode": "^1.5.0"
	},
	"categories": [
		"Languages"
	],
	"contributes": {
		"languages": [{
			"id": "lmntal",
			"aliases": ["LMNtal", "lmntal"],
			"extensions": ["lmn"],
			"configuration": "./language-configuration.json"
		}],
		"grammars": [{
			"language": "lmntal",
			"scopeName": "text.lmntal",
			"path": "./syntaxes/lmntal.tmLanguage.json"
		}]
	}
}